echo "kit of fast commands for termux,repo is available in GITHUB"



alias uyp="pkg update && pkg upgrade"
alias ppbb="proot -0 -w ~$PREFIX/bin/bash"
alias icf="ifconfig"
alias instal="pkg install"
alias c7="chmod 755 "


echo "============================================================================================================="
echo "                                               packit_list                                                   "


echo " pup, is a modified command with the power of update and upgrade de system of termux.this don't have example"

echo " ppbb, is a modified command with the power of give Proot,Proot only give virtual root.this don't have example"

echo " icf, is a modified command with the power of give the data of the internet is,recomended for new users in termux.this don't have example"

echo " instal, is a simple modified command with the funtion of install,before of the use,you need to combine the command with a pakage.example: install ENTER_YOUR_PACKAGE"

echo " c7,is a modified command with the power of give permissions to a directory or a file,before of the use of this command you need to put the name of the file/directory.example: c7 ENTER_YOUR_FILE" 

